import serial, struct

# Create a serial object and open the port
tivaPort = '/dev/serial/by-id/usb-Texas_Instruments_In-Circuit_Debug_Interface_0E2150DC-if00'
tivaBaudrate = 115200
tivaSerial = serial.Serial(port=tivaPort,baudrate=tivaBaudrate,timeout=1.0)

tivaSerial.write('c')

for x in xrange(10):
    data = struct.unpack("<B",tivaSerial.read(1)) 
    print(data[0])

# Close serial port
tivaSerial.close()
